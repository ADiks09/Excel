{
	"parser": "babel-eslint",
	"env": {
		"es6": true,
		"browser": true,
		"node": true
	},
	"rules": {
		"linebreak-style" : "off",
		"require-jsdoc" : "off",
		"no-tabs" : "off",
		"indent" : "off",
		"eol-last" : "off",
		"object-curly-spacing" : "off",
		"operator-linebreak" : "off",
		"no-debugger" : "off",
		"no-unused-vars" : "off"
	},
	"extends": ["eslint:recommended", "google"]
}